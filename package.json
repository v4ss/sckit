{
  "devDependencies": {
    "dotenv": "^16.4.1",
    "ethers": "^6.10.0",
    "fs-extra": "^11.2.0",
    "prettier": "^3.2.4",
    "prettier-plugin-solidity": "^1.3.1",
    "solc": "^0.8.24"
